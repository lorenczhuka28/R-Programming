# NAMES

variable <- 1:3
names(variable)
names(variable) <- c("Lori", "Eli", "Mira")
variable
names(variable)


# Lists can have names (name=value)
list1 <- list(a=1, b=2, c=3)
list1

# Matrices can have names (each row and each column has its own name)
matrix1 <- matrix(1:4, nrow=2, ncol=2)
dimnames(matrix1) <- list(c("a","b"), c("c", "d"))
matrix1
dimnames(matrix1)
